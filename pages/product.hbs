---
nav: product
permalink: "<%= config[nav].types.map(function(i){ return '/'+nav+'/' + (i ? i + '/' : ''); }).join('\\n') %>"
type: "<%= permalink.replace(new RegExp('^\/'+nav+'\/|\/$', 'g'), '') %>"
counter: [ 0 ]
title: "<%= ( name = config[nav].TYPES[config[nav].types.indexOf(permalink.replace(new RegExp('^\/'+nav+'\/|\/$', 'g'), ''))] ) ? name + '-' : ''  %>产品展示"
---
<div class="main">
<div class="maincontainer png fixed_width pos_center">
  <div class="twocol">
  <div class="twocol_l">
{{>about}}
  </div>
  <div class="twocol_r">
    <div class="context png">
      {{>bread}}
      <div class="clear"></div>
      <div class="prods">
        {{#eachProperty product}}
        {{#contains value.data.type ../type}}
        <div class="prod">
          <a href="{{value.filename}}"><img src="{{value.data.image}}" width="130" height="100" /></a>
          <a href="{{value.filename}}">{{value.data.title}}</a><br />
          {{value.data.description}}
        </div>
        {{/contains}}
        {{/eachProperty}}
      </div>
    </div>
  </div>
  <div class="clear"></div>
  </div>
</div>
</div>

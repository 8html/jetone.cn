<div class="sidemenu">
{{#is nav "news"}}
  <div class="sidemenu_head png">
    <h2>GPS新闻中心</h2>
    <em>GPS NEWS</em>
  </div>
  <ul class="sidemenu_body png">
    {{#eachIndex config.news.types}}
      {{#if item}}
        <li><a{{#is ../../type item}} class="sidemenu_current png"{{/is}} href="/news/{{item}}/">{{get ../../config.news.TYPES index}}</a></li>
      {{/if}}
    {{/eachIndex}}
  </ul>
  <div class="sidemenu_foot png"></div>
{{else}}
{{#is nav "product"}}
  <div class="sidemenu_head png">
    <h2>GPS产品展示</h2>
    <em>GPS PRODUCTS</em>
  </div>
  <ul class="sidemenu_body png">
    {{#eachIndex config.product.types}}
      {{#if item}}
        <li><a{{#is ../../type item}} class="sidemenu_current png"{{/is}} href="/product/{{item}}/">{{get ../../config.product.TYPES index}}</a></li>
      {{/if}}
    {{/eachIndex}}
  </ul>
  <div class="sidemenu_foot png"></div>
{{else}}
{{#is nav "solution"}}
  <div class="sidemenu_head png">
    <h2>GPS解决方案</h2>
    <em>GPS SOLUTIONS</em>
  </div>
  <ul class="sidemenu_body png">
    <li><a class="sidemenu_current png" href="/solution/">车辆管理方案</a></li>
  </ul>
  <div class="sidemenu_foot png"></div>
{{else}}
{{#is nav "download"}}
  <div class="sidemenu_head png">
    <h2>GPS软件下载</h2>
    <em>GPS DOWNLOADS</em>
  </div>
  <ul class="sidemenu_body png">
    {{#eachIndex config.download.types}}
      {{#if item}}
        <li><a{{#is ../../type item}} class="sidemenu_current png"{{/is}} href="/download/{{item}}/">{{get ../../config.download.TYPES index}}</a></li>
      {{/if}}
    {{/eachIndex}}
  </ul>
  <div class="sidemenu_foot png"></div>
{{else}}
  <div class="sidemenu_head png">
    <h2>关于我们</h2>
    <em>ABOUT US</em>
  </div>
  <ul class="sidemenu_body png">
    <li><a{{#startsWith permalink "/about/"}} class="sidemenu_current png"{{/startsWith}} href="/about/">关于我们</a></li>
    <li><a{{#startsWith permalink "/job/"}} class="sidemenu_current png"{{/startsWith}} href="/job/">职位招聘</a></li>
    <li><a{{#startsWith permalink "/contact/"}} class="sidemenu_current png"{{/startsWith}} href="/contact/">联系我们</a></li>
  </ul>
  <div class="sidemenu_foot png"></div>
{{/is}}
{{/is}}
{{/is}}
{{/is}}
</div>
{{>contact}}
